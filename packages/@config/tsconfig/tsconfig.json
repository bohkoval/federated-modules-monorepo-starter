{
  "compilerOptions": {
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noEmitOnError": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  }
}
