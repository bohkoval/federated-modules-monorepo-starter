{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "lint": {},
    "test": {},
    "test:coverage": {
      "outputs": [
        "coverage/**"
      ]
    },
    "test:watch": {
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
